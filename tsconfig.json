{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "lib": [
      "DOM",
      "ES2015",
      "ScriptHost",
      "ES5",
      "ES2015.Promise",
      "ES2016",
      "ES2016.Array.Include",
      "ES2017",
      "ES2018",
      "ES2019",
      "ESNext"
    ],
    "target": "es5",
    "outDir": "./dist",
  },
  "include": [
    "./lib/axios/index.ts"
  ],
  "exclude": [
    "./node_modules/"
  ]
}